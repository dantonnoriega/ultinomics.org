  file = {Ifland et al_2009_Refined food addiction.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/SVURFKUZ/Ifland et al_2009_Refined food addiction.pdf:application/pdf}
}

