@article{scarborough_economic_2011,
  title = {The Economic Burden of Ill Health due to Diet, Physical Inactivity, Smoking, Alcohol and Obesity in the {{UK}}: An Update to 2006\textendash{}07 {{NHS}} Costs},
  issn = {1741-3842, 1741-3850},
  shorttitle = {The Economic Burden of Ill Health due to Diet, Physical Inactivity, Smoking, Alcohol and Obesity in the {{UK}}},
  doi = {10.1093/pubmed/fdr033},
  abstract = {Background Estimates of the economic cost of risk factors for chronic disease to the NHS provide evidence for prioritization of resources for prevention and public health. Previous comparable estimates of the economic costs of poor diet, physical inactivity, smoking, alcohol and overweight/obesity were based on economic data from 1992\textendash{}93.
Methods Diseases associated with poor diet, physical inactivity, smoking, alcohol and overweight/obesity were identified. Risk factor-specific population attributable fractions for these diseases were applied to disease-specific estimates of the economic cost to the NHS in the UK in 2006\textendash{}07.
Results In 2006\textendash{}07, poor diet-related ill health cost the NHS in the UK \textsterling{}5.8 billion. The cost of physical inactivity was \textsterling{}0.9 billion. Smoking cost was \textsterling{}3.3 billion, alcohol cost \textsterling{}3.3 billion, overweight and obesity cost \textsterling{}5.1 billion.
Conclusion The estimates of the economic cost of risk factors for chronic disease presented here are based on recent financial data and are directly comparable. They suggest that poor diet is a behavioural risk factor that has the highest impact on the budget of the NHS, followed by alcohol consumption, smoking and physical inactivity.},
  language = {en},
  timestamp = {2016-04-01T20:59:41Z},
  urldate = {2016-04-01},
  journal = {Journal of Public Health},
  author = {Scarborough, Peter and Bhatnagar, Prachi and Wickramasinghe, Kremlin K. and Allender, Steve and Foster, Charlie and Rayner, Mike},
  month = may,
  year = {2011},
  keywords = {economic burden,non-communicable disease (NCD),risk factors,UK},
  pages = {fdr033},
  file = {Scarborough et al_2011_The economic burden of ill health due to diet, physical inactivity, smoking,.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/2R33GETA/Scarborough et al_2011_The economic burden of ill health due to diet, physical inactivity, smoking,.pdf:application/pdf;Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/WRFD2NHJ/pubmed.fdr033.html:text/html},
