  month = aug,
  year = {2011},
  pages = {897--916},
  file = {Mihaylova et al_2011_Review of Statistical Methods for Analysing Healthcare Resources and Costs.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/DGCGB3U5/Mihaylova et al_2011_Review of Statistical Methods for Analysing Healthcare Resources and Costs.pdf:application/pdf},
