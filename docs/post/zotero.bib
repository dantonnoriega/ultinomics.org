  file = {Kueng and Yakovlev - 2014 - How Persistent Are Consumption Habits Micro-Evide.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/ASB4IFJC/Kueng and Yakovlev - 2014 - How Persistent Are Consumption Habits Micro-Evide.pdf:application/pdf}
