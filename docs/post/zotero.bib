@article{carande-kulis_public_2007,
  title = {{Public Goods and Externalities: A Research Agenda for Public Health Economics.  [Editorial]}},
  volume = {13},
  issn = {1078-4659},
  lccn = {00124784-200703000-00024},
  shorttitle = {{Public Goods and Externalities}},
  abstract = {Among the many roles a government plays in our daily lives, protecting the public's health is one of the most conspicuous. The government provides goods and services such as registration of births and deaths, public health surveillance of disease and injury, outbreak investigations, research and education, health insurance for the poor and elderly, enforcement of laws and regulations, evaluation of health promotion programs, and assurance of a competent healthy workforce. In the past, economics in public health has almost exclusively focused on efficiency of programs through the use of cost-effectiveness or net present value measures clustered under the rubric of "economic evaluation." Efficiency measures are useful at the programmatic level. However, lack of full employment and market failures including public goods and the impact of consumers and producers actions not reflected in the markets (externalities) not only compromise efficiency but also generate health inequities. We propose an expansion of the scope of existing health economics research in an area characterized as public health economics-the study of the economic role of government in public health, particularly, but not exclusively, in supplying public goods and addressing externalities., (C) 2007 Lippincott Williams \& Wilkins, Inc.},
  language = {English.},
  timestamp = {2015-09-26T04:01:43Z},
  number = {2},
  journal = {Journal of Public Health Management},
  author = {Carande-Kulis, Vilma G. and Getzen, Thomas E. and Thacker, Stephen B. MD},
  year = {2007 March/April},
  keywords = {economic evaluation,Health Economics,public health},
  pages = {227--232},
  file = {Carande-Kulis et al_2007_Public Goods and Externalities.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/X8S5RR2P/Carande-Kulis et al_2007_Public Goods and Externalities.pdf:application/pdf}
}

