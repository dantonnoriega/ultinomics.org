@article{slack_family_2007,
  series = {How Are Low-Income Children and Families Faring A Decade after Welfare Reform? Evidence From Five Non-Experimental Panel Studies},
  title = {Family Economic Well-Being Following the 1996 Welfare Reform: {{Trend}} Data from Five Non-Experimental Panel Studies},
  volume = {29},
  issn = {0190-7409},
  shorttitle = {Family Economic Well-Being Following the 1996 Welfare Reform},
  doi = {10.1016/j.childyouth.2006.12.002},
  abstract = {This analysis summarizes trends in family economic well-being from five non-experimental, longitudinal welfare-to-work studies launched following the passage of the Personal Responsibility and Work Opportunity Reconciliation Act of 1996 (PRWORA). The studies include a sizable group of parents and other caregivers who received TANF at the point of sample selection or shortly thereafter, and share a wide range of similar measures of economic well-being. This analysis provides descriptive information on how these families are faring over time. Our results confirm what has been found by previous studies. Many families remain dependent on public benefits, and are either poor or near-poor, despite gains in some indicators of economic well-being. We caution that these aggregate statistics may mask important heterogeneity among families.},
  timestamp = {2016-11-24T23:49:56Z},
  number = {6},
  urldate = {2016-11-24},
  journal = {Children and Youth Services Review},
  author = {Slack, Kristen Shook and Magnuson, Katherine A. and Berger, Lawrence M. and Yoo, Joan and Coley, Rebekah Levine and Dunifon, Rachel and Dworsky, Amy and Kalil, Ariel and Knab, Jean and Lohman, Brenda J. and Osborne, Cynthia},
  month = jun,
  year = {2007},
  keywords = {PRWORA,TANF,Welfare reform},
