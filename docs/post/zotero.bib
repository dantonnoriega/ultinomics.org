  shorttitle = {Financial {{Incentives}} for {{Extended Weight Loss}}},
  doi = {10.1007/s11606-010-1628-y},
  abstract = {ABSTRACTBACKGROUNDPrevious efforts to use incentives for weight loss have resulted in substantial weight regain after 16 weeks.OBJECTIVETo evaluate a longer term weight loss intervention using financial incentives.DESIGNA 32-week, three-arm randomized controlled trial of financial incentives for weight loss consisting of a 24-week weight loss phase during which all participants were given a weight loss goal of 1 pound per week, followed by an 8-week maintenance phase.PARTICIPANTSVeterans who were patients at the Philadelphia Veterans Affairs Medical Center with BMIs of 30\textendash{}40.INTERVENTIONParticipants were randomly assigned to participate in either a weight-monitoring program involving a consultation with a dietician and monthly weigh-ins (control condition), or the same program with one of two financial incentive plans. Both incentive arms used deposit contracts (DC) in which participants put their own money at risk (matched 1:1), which they lost if they failed to lose weight. In one incentive arm participants were told that the period after 24 weeks was for weight-loss maintenance; in the other, no such distinction was made.MAIN MEASUREWeight loss after 32 weeks.KEY RESULTSResults were analyzed using intention-to-treat. There was no difference in weight loss between the incentive arms (P = 0.80). Incentive participants lost more weight than control participants [mean DC = 8.70 pounds, mean control = 1.17, P = 0.04, 95\% CI of the difference in means (0.56, 14.50)]. Follow-up data 36 weeks after the 32-week intervention had ended indicated weight regain; the net weight loss between the incentive and control groups was no longer significant (mean DC = 1.2 pounds, 95\% CI, -2.58\textendash{}5.00; mean control = 0.27, 95\% CI, -3.77\textendash{}4.30, P = 0.76).CONCLUSIONSFinancial incentives produced significant weight loss over an 8-month intervention; however, participants regained weight post-intervention.},
  language = {en},
  timestamp = {2016-10-23T17:43:52Z},
  number = {6},
  urldate = {2016-10-23},
  journal = {Journal of General Internal Medicine},
  author = {John, Leslie K. and Loewenstein, George and Troxel, Andrea B. and Norton, Laurie and Fassbender, Jennifer E. and Volpp, Kevin G.},
  month = jan,
  year = {2011},
  pages = {621--626},
  file = {John et al_2011_Financial Incentives for Extended Weight Loss.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/SV8MU64S/John et al_2011_Financial Incentives for Extended Weight Loss.pdf:application/pdf;Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/H39KI58X/s11606-010-1628-y.html:text/html}
}

@article{royer_incentives_2015,
  title = {Incentives, {{Commitments}}, and {{Habit Formation}} in {{Exercise}}: {{Evidence}} from a {{Field Experiment}} with {{Workers}} at a {{Fortune}}-500 {{Company}}},
