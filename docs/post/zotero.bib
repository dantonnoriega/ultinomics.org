@article{escaron_supermarket_2013,
  title = {Supermarket and Grocery Store-Based Interventions to Promote Healthful Food Choices and Eating Practices: A Systematic Review},
  volume = {10},
  issn = {1545-1151},
  shorttitle = {Supermarket and Grocery Store-Based Interventions to Promote Healthful Food Choices and Eating Practices},
  doi = {10.5888/pcd10.120156},
  abstract = {INTRODUCTION: Increasingly high rates of obesity have heightened interest among researchers and practitioners in identifying evidence-based interventions to increase access to healthful foods and beverages. Because most food purchasing decisions are made in food stores, such settings are optimal for interventions aimed at influencing these decisions. The objective of this review was to synthesize the evidence on supermarket and grocery store interventions to promote healthful food choices. METHODS: We searched PubMed through July 2012 to identify original research articles evaluating supermarket and grocery store interventions that promoted healthful food choices. We categorized each intervention by type of intervention strategy and extracted and summarized data on each intervention. We developed a scoring system for evaluating each intervention and assigned points for study design, effectiveness, reach, and availability of evidence. We averaged points for each intervention category and compared the strength of the evidence for each category. RESULTS: We identified 58 articles and characterized 33 interventions. We found 7 strategies used alone or in combination. The most frequently used strategy was the combination of point-of-purchase and promotion and advertising (15 interventions); evidence for this category was scored as sufficient. On average, of 3 points possible, the intervention categories scored 2.6 for study design, 1.1 for effectiveness, 0.3 for reach, and 2 for availability of evidence. Three categories showed sufficient evidence; 4 showed insufficient evidence; none showed strong evidence. CONCLUSION: More rigorous testing of interventions aimed at improving food and beverage choices in food stores, including their effect on diet and health outcomes, is needed.},
  language = {eng},
  timestamp = {2016-10-12T03:14:32Z},
  journal = {Preventing Chronic Disease},
  author = {Escaron, Anne L. and Meinen, Amy M. and Nitzke, Susan A. and Martinez-Donate, Ana P.},
  year = {2013},
  keywords = {Choice Behavior,Commerce,Feeding Behavior,Humans},
  pages = {E50},
  file = {Escaron et al_2013_Supermarket and grocery store-based interventions to promote healthful food.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/PXHCKQ9V/Escaron et al_2013_Supermarket and grocery store-based interventions to promote healthful food.pdf:application/pdf},
