  file = {McKenzie_2012_Beyond baseline and follow-up.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/W4K672BB/McKenzie_2012_Beyond baseline and follow-up.pdf:application/pdf;ScienceDirect Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/7P7W8P2P/S030438781200003X.html:text/html}
