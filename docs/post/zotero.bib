  author = {Belloni, Alexandre and Chernozhukov, Victor and Fern{\'a}ndez-Val, Ivan and Hansen, Chris},
  month = nov,
  year = {2013},
  keywords = {Mathematics - Statistics Theory,Statistics - Machine Learning,Statistics - Methodology},
  file = {Belloni et al. - 2013 - Program Evaluation with High-Dimensional Data.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/BGNSIWXG/Belloni et al. - 2013 - Program Evaluation with High-Dimensional Data.pdf:application/pdf}
