  file = {List et al_2015_Incentives to Eat Healthy.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/G6SK58NE/List et al_2015_Incentives to Eat Healthy.pdf:application/pdf;Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/Q7HXEA6W/papers.html:text/html}
