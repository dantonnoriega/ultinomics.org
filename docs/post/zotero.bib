  month = nov,
  year = {2005},
  keywords = {Bayesian inference,detectability,excess zeros,false negative,mixture model,observation error,sampling error,zero-inflated binomial,zero-inflated Poisson,zero inflation},
  pages = {1235--1246},
  file = {Martin et al_2005_Zero tolerance ecology.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/J5HTWIKF/Martin et al_2005_Zero tolerance ecology.pdf:application/pdf;Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/XCKMU5IK/abstract.html:text/html}
