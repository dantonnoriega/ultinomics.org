@article{dinour_food_2007,
  title = {The {{Food Insecurity}}\textendash{}{{Obesity Paradox}}: {{A Review}} of the {{Literature}} and the {{Role Food Stamps May Play}}},
  volume = {107},
  issn = {0002-8223, 1878-3570},
  shorttitle = {The {{Food Insecurity}}\textendash{}{{Obesity Paradox}}},
  doi = {10.1016/j.jada.2007.08.006},
  abstract = {During the past decade, rates of food insecurity and obesity have risen, and an association has been made between these two seemingly paradoxical states. Although this relationship has not been repeatedly seen in men, research suggests a correlation in women. Studies have not been able to consistently show a relationship in children, because findings differ based on age, race/ethnicity, household income, and sex. Several proposed hypotheses explain why a correlation between food insecurity and obesity exists in adults\textemdash{}especially women\textemdash{}but not in children. This review proposes a conceptual framework linking the Food Stamp Program and other coping strategies to the food insecurity\textendash{}obesity relationship. This link has implications for Food Stamp Program policy changes, welfare reform, and poverty prevention.},
  language = {English},
  timestamp = {2016-11-20T01:43:02Z},
