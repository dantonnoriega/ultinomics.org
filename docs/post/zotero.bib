  month = nov,
  year = {2011},
  keywords = {child malnutrition,desnutricin infantil,desviacin positiva,dviance positive,foyer,hearth,malnutrition infantile,Positive deviance,revisin sistemtica,revue systmatique,systematic review,talleres hogareos},
  pages = {1354--1366},
  file = {Bisits Bullen_2011_The positive deviance-hearth approach to reducing child malnutrition.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/AKWWG3CC/Bisits Bullen_2011_The positive deviance-hearth approach to reducing child malnutrition.pdf:application/pdf;Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/W3SM5IW4/abstract.html:text/html}
}

