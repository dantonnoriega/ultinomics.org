@article{lu_robustness_2014,
  series = {Annals Issue: Misspecification Test Methods in Econometrics},
  title = {Robustness Checks and Robustness Tests in Applied Economics},
  volume = {178, Part 1},
  issn = {0304-4076},
  doi = {10.1016/j.jeconom.2013.08.016},
  abstract = {A common exercise in empirical studies is a ``robustness check'', where the researcher examines how certain ``core'' regression coefficient estimates behave when the regression specification is modified by adding or removing regressors. If the coefficients are plausible and robust, this is commonly interpreted as evidence of structural validity. Here, we study when and how one can infer structural validity from coefficient robustness and plausibility. As we show, there are numerous pitfalls, as commonly implemented robustness checks give neither necessary nor sufficient evidence for structural validity. Indeed, if not conducted properly, robustness checks can be completely uninformative or entirely misleading. We discuss how critical and non-critical core variables can be properly specified and how non-core variables for the comparison regression can be chosen to ensure that robustness checks are indeed structurally informative. We provide a straightforward new Hausman (1978) type test of robustness for the critical core coefficients, additional diagnostics that can help explain why robustness test rejection occurs, and a new estimator, the Feasible Optimally combined GLS (FOGLeSs) estimator, that makes relatively efficient use of the robustness check regressions. A new procedure for Matlab, testrob, embodies these methods.},
  timestamp = {2017-03-19T21:20:23Z},
  urldate = {2017-03-19},
  journal = {Journal of Econometrics},
  author = {Lu, Xun and White, Halbert},
  month = jan,
  year = {2014},
  keywords = {Causal effect,Combined estimator,Conditional exogeneity,Robustness,Specification test},
  pages = {194--206},
  file = {Lu_White_2014_Robustness checks and robustness tests in applied economics.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/EFM9EX9W/Lu_White_2014_Robustness checks and robustness tests in applied economics.pdf:application/pdf;ScienceDirect Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/A8447KDQ/S0304407613001668.html:text/html}
}

@article{dodge_randomized_2013,
  title = {Randomized {{Controlled Trial}} of {{Universal Postnatal Nurse Home Visiting}}: {{Impact}} on {{Emergency Care}}},
  volume = {132},
  copyright = {Copyright \textcopyright{} 2013 by the American Academy of Pediatrics},
  issn = {0031-4005, 1098-4275},
  shorttitle = {Randomized {{Controlled Trial}} of {{Universal Postnatal Nurse Home Visiting}}},
  doi = {10.1542/peds.2013-1021M},
  abstract = {BACKGROUND AND OBJECTIVES: Although nurse home visiting has proven efficacious with small samples, scaling up to community populations with diverse families has not yet proven effective. The Durham Connects program was developed in collaboration with community leaders as a brief, universal, postnatal nurse home visiting intervention designed to screen for risk, provide brief intervention, and connect families with more intensive evidence-based services as needed. This study tested program effectiveness in reducing infant emergency medical care between birth and age 12 months.
METHODS: All 4777 resident births in Durham, North Carolina across 18 months were randomly assigned, with even birth date families to intervention and odd birth date families to control. Intervention families were offered 3 to 7 contacts between 3 and 12 weeks after birth to assess family needs and connect parents with community resources to improve infant health and well-being. Hospital records were analyzed by using an intent-to-treat design to evaluate impact among a representative subset of 549 families.
RESULTS: After demographic factors (ie, birth risk, Medicaid status, ethnicity, and single parenthood) were covaried, relative to control families, families assigned to intervention had 50\% less total emergency medical care use (mean [M] emergency department visits and hospital overnights) (Mintervention = 0.78 and Mcontrol = 1.57; P $<$ .001, effect size = 0.28) across the first 12 months of life.
CONCLUSIONS: This brief, universal, postnatal nurse home visiting program improves population-level infant health care outcomes for the first 12 months of life. Nurse home visiting can be implemented universally at high fidelity with positive impacts on infant emergency health care that are similar to those of longer, more intensive home visiting programs. This approach offers a novel solution to the paradox of targeting by offering individually tailored intervention while achieving population-level impact.},
  language = {en},
  timestamp = {2017-03-18T19:19:42Z},
  number = {Supplement 2},
  urldate = {2017-03-18},
  journal = {Pediatrics},
  author = {Dodge, Kenneth A. and Goodman, W. Benjamin and Murphy, Robert A. and O'Donnell, Karen and Sato, Jeannine},
  month = nov,
  year = {2013},
  keywords = {child injuries,home visiting,prevention},
  pages = {S140--S146},
  file = {Dodge et al_2013_Randomized Controlled Trial of Universal Postnatal Nurse Home Visiting.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/QAFR75IH/Dodge et al_2013_Randomized Controlled Trial of Universal Postnatal Nurse Home Visiting.pdf:application/pdf;Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/JT6SQ2G8/S140.html:text/html},
  pmid = {24187116}
}

