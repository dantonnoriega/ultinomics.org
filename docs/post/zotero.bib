  keywords = {demand elasticity,ELASTICITY (Economics),FCDS model,Food demand,FOOD industry,food prices,Household food demand,income,INDIA,MATHEMATICAL models,QUAIDS model,RESEARCH},
  pages = {1--14},
  file = {Kumar et al. - 2011 - Estimation of Demand Elasticity for Food Commoditi.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/GKN5JT2M/Kumar et al. - 2011 - Estimation of Demand Elasticity for Food Commoditi.pdf:application/pdf}
