@article{grindal_does_2016,
  title = {Does Food Retail Access Moderate the Impact of Fruit and Vegetable Incentives for {{SNAP}} Participants? {{Evidence}} from Western {{Massachusetts}}},
  volume = {61},
  issn = {0306-9192},
  shorttitle = {Does Food Retail Access Moderate the Impact of Fruit and Vegetable Incentives for {{SNAP}} Participants?},
  doi = {10.1016/j.foodpol.2016.02.002},
  abstract = {Objectives
This study investigates whether the response of Supplemental Nutrition Assistance Program (SNAP) participants to a 30\% incentive on fruit and vegetable spending varies with their access to food retailers.
Methods
The analysis exploits the random assignment of SNAP households in Hampden County, MA, to an intervention group that earned the incentive. Regression models for the impact of the incentive are augmented with measures of food retail access and interactions of random assignment status with food retail access. The main outcome\textemdash{}use of the SNAP benefit\textemdash{}is based on Electronic Benefit Transfer (EBT) card transaction records.
Results
Although households that lived within a mile of a participating supermarket spent approximately \$2.13 or 19\% more per month on targeted fruits and vegetables at participating supermarkets than households that did not live within a mile of a participating supermarket, we found no evidence that the impact of the incentive on SNAP fruit and vegetable spending varies with distance to retailers.
Conclusions
These findings imply that incentives to purchase fruits and vegetables were equally efficacious for SNAP households with high and low access to food retailers.},
  timestamp = {2016-10-25T20:15:42Z},
  urldate = {2016-10-23},
  journal = {Food Policy},
  author = {Grindal, Todd and Wilde, Parke and Schwartz, Gabe and Klerman, Jacob and Bartlett, Susan and Berman, Danielle},
  month = may,
  year = {2016},
  keywords = {Food retail,fruits and vegetables,Healthy Incentives Pilot,Incentives,Spatial analysis,Supplemental Nutrition Assistance Program},
  pages = {59--69},
  file = {Grindal et al_2016_Does food retail access moderate the impact of fruit and vegetable incentives.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/2MVNTRS4/Grindal et al_2016_Does food retail access moderate the impact of fruit and vegetable incentives.pdf:application/pdf;ScienceDirect Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/EITUKSG2/S0306919216000208.html:text/html}
