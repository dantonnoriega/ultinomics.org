@article{epstein_experimental_2012,
  title = {Experimental Research on the Relation between Food Price Changes and Food-Purchasing Patterns: A Targeted Review},
  volume = {95},
  issn = {0002-9165, 1938-3207},
  shorttitle = {Experimental Research on the Relation between Food Price Changes and Food-Purchasing Patterns},
  doi = {10.3945/ajcn.111.024380},
  abstract = {One way in which to modify food purchases is to change prices through tax policy, subsidy policy, or both. We reviewed the growing body of experimental research conducted in the laboratory and in the field that investigates the following: the extent to which price changes influence purchases of targeted and nontargeted foods, total energy, or macronutrients purchased; the interaction of price changes with adjunctive interventions; and moderators of sensitivity to price changes. After a brief overview of economic principles and observational research that addresses these issues, we present a targeted review of experimental research. Experimental research suggests that price changes modify purchases of targeted foods, but research on the overall nutritional quality of purchases is mixed because of substitution effects. There is mixed support for combining price changes with adjunctive interventions, and there are no replicated findings on moderators to price sensitivity in experiments. Additional focused research is needed to better inform food policy development with the aim of improving eating behavior and preventing obesity.},
  language = {en},
  timestamp = {2016-11-20T17:12:38Z},
  number = {4},
  urldate = {2016-11-20},
  journal = {The American Journal of Clinical Nutrition},
  author = {Epstein, Leonard H. and Jankowiak, Noelle and Nederkoorn, Chantal and Raynor, Hollie A. and French, Simone A. and Finkelstein, Eric},
  month = apr,
  year = {2012},
  pages = {789--809},
  file = {Epstein et al_2012_Experimental research on the relation between food price changes and.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/W6SAPD5F/Epstein et al_2012_Experimental research on the relation between food price changes and.pdf:application/pdf;Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/GIDX5ZEV/789.html:text/html},
