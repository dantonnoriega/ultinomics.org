  number = {11},
  urldate = {2015-09-23},
  journal = {Archives of Internal Medicine},
  author = {{Donnelly JE} and {Hill JO} and {Jacobsen DJ} and {et al}},
  month = jun,
  year = {2003},
  pages = {1343--1350},
  file = {Donnelly JE et al_2003_Effects of a 16-month randomized controlled exercise trial on body weight and.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/888KDQ33/Donnelly JE et al_2003_Effects of a 16-month randomized controlled exercise trial on body weight and.pdf:application/pdf}
}

