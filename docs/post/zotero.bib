@article{galle_carrots_2013,
  title = {Carrots, {{Sticks}}, and {{Salience}}},
  volume = {67},
  copyright = {Copyright New York University School of Law, Tax Law Office Fall 2013},
  issn = {00400041},
  abstract = {Money is usually a good motivator. When market actors buy or sell things that have unfortunate consequences for others -- negative externalities -- a standard economic prescription is to raise the price of the externality-producing good. Alternatively, the bad actor can be paid to stop. For policymakers, choosing between these two options, the stick and the carrot, can be a difficult task even if people respond as expected to cash incentives. David Gamage and Darien Shanske argue that low-salience taxes can be just as effective at changing behavior if policymakers increase their price to make up for the taxes' low visibility. Carrots are not necessarily always inferior to sticks, once salience and its potential correlations are taken into account. Fully salient price instruments may not always be ideal. Government should likely adjust the prices of its taxes and subsidies at least to account for the process of learning and diffusion of information across the economy. And these adjustments may be closer to optimal if they balance revenue and other needs against the correction of externalities.},
  language = {English},
  timestamp = {2015-11-10T16:23:13Z},
  number = {1},
  urldate = {2015-11-10},
  journal = {Tax Law Review},
  author = {Galle, Brian},
  year = {Fall 2013},
  keywords = {Behavior,Business And Economics,Law,Motivation,Policy making,Studies,Taxation},
  pages = {53--109},
