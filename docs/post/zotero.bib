@article{meyer_viewpoint:_2011,
  title = {Viewpoint: {{Further}} Results on Measuring the Well-Being of the Poor Using Income and Consumption},
  volume = {44},
  copyright = {Copyright \textcopyright{} 2011 Canadian Economics Association},
  issn = {0008-4085},
  shorttitle = {Viewpoint},
  abstract = {We evaluate the relative merits of income-and consumption-based measures of well-being. Our results provide evidence that consumption better captures well-being for those with few resources. The bottom deciles of expenditures exceed those of income, suggesting under-reporting of income. The under-reporting rate for government transfers is high and rising. Overall non-response is more severe in U. S. income data than in expenditure data. Furthermore, a consumption data set requires fewer observations than an income data set to obtain the same level of precision for typical estimates. Finally, very low consumption is more strongly related to other bad outcomes than very low income. On {\'e}value les m{\'e}rites relatifs des mesures de bien-{\^e}tre fond{\'e}es sur le revenu et la consommation. Les r{\'e}sultats montrent que la consommation mesure mieux le bien-{\^e}tre de ceux qui ont peu de ressources. Les d{\'e}ciles au bas de la distribution des d{\'e}penses d{\'e}passent les d{\'e}ciles de la distribution des revenus -ce qui sugg{\`e}re qu'il y aurait des revenus non rapport{\'e}s. Le taux de sous-d{\'e}claration des transferts gouvernementaux est {\'e}lev{\'e} et croissant. Le taux global de non-r{\'e}ponses est plus important aux Etats-Unis pour les revenus que pour les d{\'e}penses. De plus, les bases de donn{\'e}es sur la consommation requi{\`e}rent moins d'observations que celles sur le revenu pour obtenir le m{\^e}me degr{\'e} de pr{\'e}cision des {\'e}valuations typiques. Finalement, une tr{\`e}s basse consommation est plus fortement co-reli{\'e}e {\`a} d'autres mauvaises conditions qu'un tr{\`e}s bas niveau de revenu.},
  timestamp = {2014-01-20T00:03:50Z},
  number = {1},
  urldate = {2014-01-20},
  journal = {The Canadian Journal of Economics / Revue canadienne d'Economique},
  author = {Meyer, Bruce D. and Sullivan, James X.},
  month = feb,
  year = {2011},
  keywords = {econ},
  pages = {52--87},
  file = {Meyer and Sullivan - 2011 - Viewpoint Further results on measuring the well-b.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/J4J2EDAV/Meyer and Sullivan - 2011 - Viewpoint Further results on measuring the well-b.pdf:application/pdf}
}

