  journal = {The Journal of Nutrition},
  author = {Vossenaar, Marieke and Mayorga, Evelyn and Soto-M{\'e}ndez, Mar{\'\i}a Jos{\'e} and Medina-Monchez, Susana B. and Campos, Raquel and Anderson, Annie S. and Solomons, Noel W.},
  month = jan,
  year = {2009},
  pages = {755--762},
  file = {Vossenaar et al_2009_The Positive Deviance Approach Can Be Used to Create Culturally Appropriate.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/SG3PXFHE/Vossenaar et al_2009_The Positive Deviance Approach Can Be Used to Create Culturally Appropriate.pdf:application/pdf},
