  abstract = {Retailers have long recognized the importance of business analytics methods for facilitating more effective customer-related decisions that result in added-value customer service. We recommend a retail data-driven approach that reveals a wealth of consumer shopping behavioral insights. We employ clustering techniques to segment the customer visits. The available customer visit segmentation studies view shoppers and their visits just as associations of product items. On the contrary, we discover patterns in the shoppers' shopping habits to comprehend the customers' behavior behind their visits and sales. Our approach segments customer visits and translates them to customer shopping behavior. We demonstrate the utility of this approach by applying it to real data provided by a Fortune 500 specialty retailer of home improvement and construction products. Apart from its theoretical contribution, our approach extracts knowledge that may support several decisions ranging from marketing campaigns per customer segment, redesign of store's layout to product recommendations.},
  timestamp = {2016-11-13T19:25:09Z},
  number = {ID 2821649},
  urldate = {2016-11-13},
  institution = {Social Science Research Network},
  author = {Griva, Anastasia and Bardaki, Cleopatra and Papakiriakopoulos, Dimitris and Pramatari, Katerina},
  month = may,
  year = {2016},
  keywords = {Clustering,Customer Behavior,Retail Analytics,Visits Segmentation},
  file = {Griva et al_2016_Framing Customer Shopping Behavior Through Retail Data Analytics.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/5Z4C5G7M/Griva et al_2016_Framing Customer Shopping Behavior Through Retail Data Analytics.pdf:application/pdf;Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/KN2RXKIV/papers.html:text/html}
