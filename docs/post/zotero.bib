  keywords = {Adult,Aged,Econometric,Environment Design,Fast Foods,Female,Food Supply,Humans,Indiana,Least-Squares Analysis,Male,Middle Aged,Models,obesity,Overweight,Residence Characteristics,Urban Population,young adult},
