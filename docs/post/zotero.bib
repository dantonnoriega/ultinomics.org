  file = {Anderson_2001_Permutation tests for univariate or multivariate analysis of variance and.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/CQC2X5UJ/Anderson_2001_Permutation tests for univariate or multivariate analysis of variance and.pdf:application/pdf;Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/CE2F6XI2/f01-004.html:text/html}
}

