@article{galvao_estimation_2013,
  title = {Estimation of {{Censored Quantile Regression}} for {{Panel Data With Fixed Effects}}},
  volume = {108},
  issn = {0162-1459},
  doi = {10.1080/01621459.2013.818002},
  abstract = {This article investigates estimation of censored quantile regression (QR) models with fixed effects. Standard available methods are not appropriate for estimation of a censored QR model with a large number of parameters or with covariates correlated with unobserved individual heterogeneity. Motivated by these limitations, the article proposes estimators that are obtained by applying fixed effects QR to subsets of observations selected either parametrically or nonparametrically. We derive the limiting distribution of the new estimators under joint limits, and conduct Monte Carlo simulations to assess their small sample performance. An empirical application of the method to study the impact of the 1964 Civil Rights Act on the black\textendash{}white earnings gap is considered. Supplementary materials for this article are available online.},
  timestamp = {2017-01-03T22:57:07Z},
  number = {503},
  urldate = {2017-01-03},
  journal = {Journal of the American Statistical Association},
  author = {Galvao, Antonio F. and Lamarche, Carlos and Lima, Luiz Renato},
  month = sep,
  year = {2013},
  keywords = {Civil right,Earnings gap,Fixed censoring,Individual heterogeneity,Longitudinal data},
  pages = {1075--1089},
