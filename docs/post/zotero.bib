  file = {Dharmasena et al_2011_Partial versus General Equilibrium Calorie and Revenue Effects of a.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/RMBKNB73/Dharmasena et al_2011_Partial versus General Equilibrium Calorie and Revenue Effects of a.pdf:application/pdf;RePEc Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/61pi1cm2.default/zotero/storage/HRA7WWS7/103509.html:text/html}
